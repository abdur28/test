<%- include("partials/header") %>
  <!-- Content -->
  <div class="container mx-auto">
    <div class="grid grid-cols-2 gap-16">
      <section class="col-span-2 md:col-span-1">
        <div class="max-w-screen-md">
          <h1 class="font-signika text-4xl pt-10 pb-8">ADMIN</h1>

          <form class="space-y-8" action="/iamtheowner01-admin" method="post" enctype="application/x-www-form-urlencoded">
            <p class="text-2xl pt-1 pb-8">Contact</p>
            <div>
              <label for="address"
                class="block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300">Address</label>
              <input type="text" id="address" value="<%= adminInfo.address %>" name="address"
                class="block p-3 w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 shadow-sm focus:ring-primary-500 focus:border-primary-500 dark:bg-neutral-900 dark:border-neutral-800 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500 dark:shadow-sm-light"
                placeholder="Your address" />
            </div>
            <div>
                <label for="number"
                  class="block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300">Phone Number</label>
                <input type="text" id="number" value="<%= adminInfo.number %>" name="number"
                  class="block p-3 w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 shadow-sm focus:ring-primary-500 focus:border-primary-500 dark:bg-neutral-900 dark:border-neutral-800 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500 dark:shadow-sm-light"
                  placeholder="Your Phone Number" />
            </div>
            <div>
                <label for="email"
                  class="block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300">Email</label>
                <input type="email" id="email" value="<%= adminInfo.email %>" name="email"
                  class="block p-3 w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 shadow-sm focus:ring-primary-500 focus:border-primary-500 dark:bg-neutral-900 dark:border-neutral-800 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500 dark:shadow-sm-light"
                  placeholder="Your Email" />
            </div>

            <p class="text-2xl pt-1 pb-8">Social media</p>
            <div>
                <label for="instagram"
                  class="block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300">Instagram</label>
                <input type="text" id="instagram" name="instagram" value="<%= adminInfo.instagram %>"
                  class="block p-3 w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 shadow-sm focus:ring-primary-500 focus:border-primary-500 dark:bg-neutral-900 dark:border-neutral-800 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500 dark:shadow-sm-light"
                  placeholder="@Instagram" />
            </div>
            <div>
                <label for="twitter"
                  class="block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300">Twitter</label>
                <input type="text" id="twitter" name="twitter" value="<%= adminInfo.twitter %>"
                  class="block p-3 w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 shadow-sm focus:ring-primary-500 focus:border-primary-500 dark:bg-neutral-900 dark:border-neutral-800 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500 dark:shadow-sm-light"
                  placeholder="@Twitter" />
            </div>
            <div>
              <label for="telegram"
                class="block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300">Telegram</label>
              <input type="text" id="telegram" name="telegram" value="<%= adminInfo.telegram %>"
                class="block p-3 w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 shadow-sm focus:ring-primary-500 focus:border-primary-500 dark:bg-neutral-900 dark:border-neutral-800 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500 dark:shadow-sm-light"
                placeholder="@Telegram" />
            </div>
            <div>
              <label for="facebook"
                class="block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300">Facebook</label>
              <input type="text" id="facebook" name="facebook" value="<%= adminInfo.facebook %>"
                class="block p-3 w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 shadow-sm focus:ring-primary-500 focus:border-primary-500 dark:bg-neutral-900 dark:border-neutral-800 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500 dark:shadow-sm-light"
                placeholder="Facebook" />
            </div>

            <p class="text-2xl pt-1 pb-8">About Me</p>
            <div>
                <label for="intro"
                  class="block mb-2 text-sm font-medium text-gray-900 dark:text-gray-300">Intro</label>
                <input type="text" id="intro" name="aboutMeInfo" value="<%= adminInfo.aboutMeInfo %>"
                  class="block p-3 w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 shadow-sm focus:ring-primary-500 focus:border-primary-500 dark:bg-neutral-900 dark:border-neutral-800 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500 dark:shadow-sm-light"
                  placeholder="Your Intro" />
            </div>
            <div class="sm:col-span-2">
                <label for="bio" class="block mb-2 text-sm font-medium text-gray-900 dark:text-gray-400">Bio</label>
                <textarea id="bio" rows="20" name="bio" 
                  class="block p-2.5 w-full text-sm text-gray-900 bg-gray-50 rounded-lg shadow-sm border border-gray-300 focus:ring-primary-500 focus:border-primary-500 dark:bg-neutral-900 dark:border-neutral-800 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500"
                  placeholder="<%= adminInfo.bio %>" ></textarea>
            </div>


